<?xml version='1.0' encoding='UTF-8'?>
<hudson>
  <disabledAdministrativeMonitors/>
  <version>1.643</version>
  <numExecutors>2</numExecutors>
  <mode>NORMAL</mode>
  <useSecurity>true</useSecurity>
  <authorizationStrategy class="hudson.security.GlobalMatrixAuthorizationStrategy">
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Create:{{ user.handle }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Create:jenkins-users</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Delete:{{ user.handle }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Delete:jenkins-users</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.ManageDomains:{{ user.handle }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.ManageDomains:jenkins-users</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Update:{{ user.handle }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Update:jenkins-users</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.View:{{ user.handle }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.View:jenkins-users</permission>
    <permission>hudson.model.Computer.Build:{{ user.handle }}</permission>
    <permission>hudson.model.Computer.Build:jenkins-users</permission>
    <permission>hudson.model.Computer.Configure:{{ user.handle }}</permission>
    <permission>hudson.model.Computer.Configure:jenkins-users</permission>
    <permission>hudson.model.Computer.Connect:{{ user.handle }}</permission>
    <permission>hudson.model.Computer.Connect:jenkins-users</permission>
    <permission>hudson.model.Computer.Create:{{ user.handle }}</permission>
    <permission>hudson.model.Computer.Create:jenkins-users</permission>
    <permission>hudson.model.Computer.Delete:{{ user.handle }}</permission>
    <permission>hudson.model.Computer.Delete:jenkins-users</permission>
    <permission>hudson.model.Computer.Disconnect:{{ user.handle }}</permission>
    <permission>hudson.model.Computer.Disconnect:jenkins-users</permission>
    <permission>hudson.model.Hudson.Administer:{{ user.handle }}</permission>
    <permission>hudson.model.Hudson.Administer:jenkins-users</permission>
    <permission>hudson.model.Hudson.ConfigureUpdateCenter:{{ user.handle }}</permission>
    <permission>hudson.model.Hudson.ConfigureUpdateCenter:jenkins-users</permission>
    <permission>hudson.model.Hudson.Read:{{ user.handle }}</permission>
    <permission>hudson.model.Hudson.Read:jenkins-users</permission>
    <permission>hudson.model.Hudson.RunScripts:{{ user.handle }}</permission>
    <permission>hudson.model.Hudson.RunScripts:jenkins-users</permission>
    <permission>hudson.model.Hudson.UploadPlugins:{{ user.handle }}</permission>
    <permission>hudson.model.Hudson.UploadPlugins:jenkins-users</permission>
    <permission>hudson.model.Item.Build:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Build:jenkins-users</permission>
    <permission>hudson.model.Item.Cancel:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Cancel:jenkins-users</permission>
    <permission>hudson.model.Item.Configure:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Configure:jenkins-users</permission>
    <permission>hudson.model.Item.Create:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Create:jenkins-users</permission>
    <permission>hudson.model.Item.Delete:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Delete:jenkins-users</permission>
    <permission>hudson.model.Item.Discover:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Discover:jenkins-users</permission>
    <permission>hudson.model.Item.Read:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Read:jenkins-users</permission>
    <permission>hudson.model.Item.Workspace:{{ user.handle }}</permission>
    <permission>hudson.model.Item.Workspace:jenkins-users</permission>
    <permission>hudson.model.Run.Delete:{{ user.handle }}</permission>
    <permission>hudson.model.Run.Delete:jenkins-users</permission>
    <permission>hudson.model.Run.Update:{{ user.handle }}</permission>
    <permission>hudson.model.Run.Update:jenkins-users</permission>
    <permission>hudson.model.View.Configure:{{ user.handle }}</permission>
    <permission>hudson.model.View.Configure:jenkins-users</permission>
    <permission>hudson.model.View.Create:{{ user.handle }}</permission>
    <permission>hudson.model.View.Create:jenkins-users</permission>
    <permission>hudson.model.View.Delete:{{ user.handle }}</permission>
    <permission>hudson.model.View.Delete:jenkins-users</permission>
    <permission>hudson.model.View.Read:{{ user.handle }}</permission>
    <permission>hudson.model.View.Read:jenkins-users</permission>
    <permission>hudson.scm.SCM.Tag:{{ user.handle }}</permission>
    <permission>hudson.scm.SCM.Tag:jenkins-users</permission>
  </authorizationStrategy>
  <securityRealm class="hudson.security.LDAPSecurityRealm" plugin="ldap@1.11">
    <server>ldap://{{ production_jails.openldap.ip }}</server>
    <rootDN>dc={{ external_domain.split('.') | join(',dc=') }}</rootDN>
    <inhibitInferRootDN>false</inhibitInferRootDN>
    <userSearchBase>ou=people</userSearchBase>
    <userSearch>uid={0}</userSearch>
    <groupSearchBase>cn=users</groupSearchBase>
    <groupSearchFilter>(&amp; (cn={0}) (objectclass=posixGroupOfNames))</groupSearchFilter>
    <groupMembershipStrategy class="jenkins.security.plugins.ldap.FromUserRecordLDAPGroupMembershipStrategy"/>
    <managerDN>cn=Manager,dc={{ external_domain.split('.') | join(',dc=') }}</managerDN>
    <managerPasswordSecret>{{ jenkins_ldap_hash.stdout }}</managerPasswordSecret>
    <disableMailAddressResolver>true</disableMailAddressResolver>
    <displayNameAttributeName>givenname</displayNameAttributeName>
    <mailAddressAttributeName>mail</mailAddressAttributeName>
    <userIdStrategy class="jenkins.model.IdStrategy$CaseInsensitive"/>
    <groupIdStrategy class="jenkins.model.IdStrategy$CaseInsensitive"/>
  </securityRealm>
  <disableRememberMe>false</disableRememberMe>
  <projectNamingStrategy class="jenkins.model.ProjectNamingStrategy$DefaultProjectNamingStrategy"/>
  <workspaceDir>${JENKINS_HOME}/workspace/${ITEM_FULLNAME}</workspaceDir>
  <buildsDir>${ITEM_ROOTDIR}/builds</buildsDir>
  <markupFormatter class="hudson.markup.EscapedMarkupFormatter"/>
  <jdks/>
  <viewsTabBar class="hudson.views.DefaultViewsTabBar"/>
  <myViewsTabBar class="hudson.views.DefaultMyViewsTabBar"/>
  <clouds/>
  <quietPeriod>5</quietPeriod>
  <scmCheckoutRetryCount>0</scmCheckoutRetryCount>
  <views>
    <hudson.model.AllView>
      <owner class="hudson" reference="../../.."/>
      <name>All</name>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
    </hudson.model.AllView>
  </views>
  <primaryView>All</primaryView>
  <slaveAgentPort>0</slaveAgentPort>
  <label></label>
  <nodeProperties/>
  <globalNodeProperties/>
  <noUsageStatistics>true</noUsageStatistics>
</hudson>
